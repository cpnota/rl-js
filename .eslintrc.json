{
  "parser": "babel-eslint",
  "extends": [
      "airbnb"
  ],
  "plugins": [
      "jest"
  ],
  "env": {
      "browser": true,
      "jest/globals": true
  },
  "rules": {
      "class-methods-use-this": "off",
      "global-require": "off",
      "react/jsx-one-expression-per-line": "off"
  }
}
